{
  "private": true,
  "name": "headless-motion",
  "version": "0.0.1",
  "scripts": {
    "dev": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "main": "dist/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "dumi": "^1.0.13",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.1.8",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "ahooks": "^3.7.0",
    "framer-motion": "^6.3.3",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
