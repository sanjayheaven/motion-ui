{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "dumi": "^1.0.13",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "ahooks": "^3.7.0",
    "framer-motion": "^6.3.3"
  }
}
